{
  "name": "react-redux-boilerplate-greyatom",
  "version": "0.0.1",
  "description": "web-app",
  "main": "index.js",
  "scripts": {
    "test": "npm run check-deps && NODE_ENV=production mocha --require moch_setup.js './src/**/*.test.js' --compilers js:babel-core/register --recursive",
    "lint": "eslint './src/**/*.js' './tests/**/*.js'",
    "start": "npm run check-deps && node devServer.js",
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run check-deps && npm run build:webpack",
    "check-deps": "npm run check-npm-deps && npm run check-bower-deps",
    "check-npm-deps": "check-dependencies --package-manager npm",
    "check-bower-deps": "check-dependencies --package-manager bower"
  },
  "pre-push": [
    "test",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:greyatom-edu-tech/react-redux-boilerplate.git"
  },
  "author": "GreyAtom",
  "license": "ISC",
  "homepage": "https://github.com/greyatom-edu-tech/react-redux-boilerplate",
  "dependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "enzyme": "^2.4.1",
    "es6-promise": "^3.2.1",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "isomorphic-fetch": "^2.2.1",
    "localforage": "^1.4.2",
    "lodash": "^4.15.0",
    "raven-js": "^3.7.0",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.3",
    "react-custom-scrollbars": "^4.0.0",
    "react-dom": "^15.3.0",
    "react-highcharts": "^10.0.0",
    "react-localization": "0.0.6",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "react-slick": "^0.14.5",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "validator": "^6.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-register": "^6.11.6",
    "check-dependencies": "^1.0.1",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-import-resolver-babel-module": "^2.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "jsdom": "^9.11.0",
    "mocha": "^3.0.2",
    "nock": "^8.0.0",
    "node-jsdom": "^3.1.5",
    "pre-push": "^0.1.1",
    "react-addons-test-utils": "^15.3.1",
    "redux-mock-store": "^1.1.4",
    "sinon": "^1.17.6"
  }
}
